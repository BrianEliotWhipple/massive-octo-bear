{
  "id": "/example",
  "groups": [
    {
      "id": "/example/db",
      "apps": [
        {
          "id": "/example/db/cassandra",
          "container": {
            "type": "DOCKER",
            "docker": {
              "image": "cassandra:2.1",
              "network": "BRIDGE",
              "portMappings": [
                {
                  "containerPort": 9042,
                  "hostPort": 9042,
                  "servicePort": 0,
                  "protocol": "tcp"
                }
              ],
              "privileged": false,
              "parameters": [
              ]
            },
            "volumes": [ ]
          },
          "cpus": 1.0,
          "mem": 512,
          "deployments": [ ],
          "env": { },
          "executor": "",
          "constraints": [ ],
          "labels": { },
          "healthChecks": [ ],
          "instances": 1,
          "ports": [ ],
          "backoffSeconds": 1,
          "backoffFactor": 1.15,
          "maxLaunchDelaySeconds": 3600,
          "tasksRunning": 1,
          "tasksHealthy": 1,
          "tasksUnhealthy": 0,
          "tasksStaged": 0,
          "uris": [],
          "dependencies": [ ],
          "upgradeStrategy": {
            "minimumHealthCapacity": 0.5,
            "maximumOverCapacity": 0.2
          },
          "version": "1"
        }
      ]
    },{
      "id": "/example/service",
      "dependencies": ["/example/db"],
      "apps": [
        {
          "id": "/example/service/echo",
          "container": {
            "type": "DOCKER",
            "docker": {
              "image": "172.17.8.12:5000/example/java-echo:0.1",
              "network": "BRIDGE",
              "portMappings": [
                {
                  "containerPort": 8080,
                  "hostPort": 31000,
                  "servicePort": 0,
                  "protocol": "tcp"
                },
                {
                  "containerPort": 8081,
                  "hostPort": 31001,
                  "servicePort": 0,
                  "protocol": "tcp"
                }
              ],
              "privileged": false,
              "parameters": [
                { "key": "env", "value": "CASSANDRA_SEED_HOST=172.17.8.10" }
              ]
            },
            "volumes": [ ]
          },
          "cpus": 1.0,
          "mem": 512,
          "deployments": [ ],
          "env": { },
          "executor": "",
          "constraints": [ ],
          "labels": { },
          "healthChecks": [
            {
              "protocol": "HTTP",
              "portIndex": 1,
              "path": "/healthcheck",
              "gracePeriodSeconds": 300,
              "intervalSeconds": 60,
              "maxConsecutiveFailures": 3
            }
          ],
          "instances": 1,
          "ports": [],
          "backoffSeconds": 1,
          "backoffFactor": 1.15,
          "maxLaunchDelaySeconds": 3600,
          "tasksRunning": 1,
          "tasksHealthy": 1,
          "tasksUnhealthy": 0,
          "tasksStaged": 0,
          "uris": [],
          "dependencies": [
            "/example/db/cassandra"
          ],
          "upgradeStrategy": {
            "minimumHealthCapacity": 0.5,
            "maximumOverCapacity": 0.2
          },
          "version": "1"
        }
      ]
    }
  ]
}
